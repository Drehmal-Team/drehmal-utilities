{
"env": {
    "gameVersion": "1.21.3"
 },
 "lint": {
    "undeclaredSymbol": [
        {
            "if": [
                {
                    "pattern": "ntils.(z.(const|global|singularity.flag)|API)"
                }
            ],
            "then": {
                "declare": "block"
            }
        },
        {
            "if": [
                {
                    "pattern": "(ntils:api/singularity/error|ntils:api/uuid)"
                }
            ],
            "then": {
                "declare": "block"
            }
        },
        {
            "if": [
                {
                    "category": ["score_holder", "dimension", "tag"]
                }
            ],
            "then": {
                "declare": "block"
            }
        },

        {
            "then": { "report": "warning" }
        }
    ]
 }
}